  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "samples": "ng serve samples",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "gulp docs",
    "ins": "npm install @logo-software-${npm_config_lib}-*.*.tgz",
    "icons": "npm run icons --prefix projects/logo-software/icons/",
    "w": "ng build @logo-software/${npm_config_lib} --prod --watch",
    "b": "ng build @logo-software/${npm_config_lib} --prod",
    "link": "npm link @logo-software/core @logo-software/cursor @logo-software/excel @logo-software/icons @logo-software/language @logo-software/paging @logo-software/perspective @logo-software/table @logo-software/theme @logo-software/toast @logo-software/carousel @logo-software/links @logo-software/combobox",
    "link-developer.logo.com.tr": "cd projects/developer.logo.com.tr && npm run link",
    "######################### Library": "######",
    "!": "run m --project=developer.logo.com.tr ile yeni bir module yarat. Sana ismi soracaktır. Oraya services/ss girince okydir",
    "m": "ng g m --project=${npm_config_project} --lintFix=true --routing --module && ng g c --project=${npm_config_project} --export=true --module --prefix=logo --lintFix=true --inlineTemplate=false --inlineStyle=false --style=scss",
    "create-sample": "ng g m logo/${npm_config_lib}-sample --project=samples --routing && ng g c logo/${npm_config_lib}-sample/${npm_config_lib}-showcase --project=samples --export=true --module --prefix=logo --lintFix=true --inlineTemplate=false --inlineStyle=false --style=scss",
    "cl": "ng g library @logo-software/${npm_config_lib} --lintFix=true --prefix=logo",
    "c": "npm run cl --lib=${npm_config_lib} && npm run create-sample --lib=${npm_config_lib}",
    "######################### Versioning": "###### sample usage: npm run patch --lib=table ######",
    "patch": "npm run patch:commit --prefix projects/logo-software/${npm_config_lib}/",
    "minor": "npm run minor:commit --prefix projects/logo-software/${npm_config_lib}/",
    "pre": "npm run pre:commit --prefix projects/logo-software/${npm_config_lib}/",
    "major": "npm run major:commit --prefix projects/logo-software/${npm_config_lib}/",
    "pack": "ng build @logo-software/${npm_config_lib} --prod &&  npm pack dist/logo-software/${npm_config_lib}",
    "pub": "rm -rf logo-software-${npm_config_lib}-*.*.tgz && npm run pack --lib=${npm_config_lib} && npm publish --access public logo-software-${npm_config_lib}-*.*.tgz",
    "upub": "npm run unpublish --prefix projects/logo-software/${npm_config_lib}/",
    "######################### Docs": "######",
    "ver:patch": "npm --no-git-tag-version version patch",
    "ver:minor": "npm version minor --no-git-tag-version",
    "ver:pre": "npm version prerelease --no-git-tag-version",
    "ver:major": "npm version major --no-git-tag-version",
    "postversion": "echo '#########\n### Versioning to:' $npm_package_version '\n### CommandPATH: ' $INIT_CWD '\n#########' && git status",
    "git-commit": "git add -A . && git commit . -m $TYPE'('$npm_package_name'): upgrading to v.'$npm_package_version",
    "git-release": "git add -A . && git commit . -m $TYPE'('$npm_package_name'): cut the v.'$npm_package_version' release'",
    "doc:patch": "npm run ver:patch && npm run build && TYPE=fix npm run git-commit",
    "doc:minor": "npm run ver:minor && npm run build && TYPE=feat npm run git-commit",
    "doc:pre": "npm run ver:pre && npm run build && TYPE=prerelease npm run git-commit",
    "doc:major": "npm run ver:major && TYPE=release npm run git-release",
    "######################### Sample Commands": "#####",
    "sample_prefixAnotherPackage": "npm run build --prefix projects/logo-software/table/",
    "sample_path": "echo '######### build' && echo $INIT_CWD && echo $npm_execpath",
    "sample_build": "echo '######### build' && echo $INIT_CWD && echo $npm_execpath && node src/lib/assets/scripts/install.js",
    "sample_preversion": "npm test",
    "sample_version": "npm run build && git add -f -A ../../../dist/logo-software/table",
    "sample_postversion": "git push && git push --tags && rm -rf build/temp",
    "sample_install": "src/lib/assets/scripts/install.js",
    "sample_postinstall": "echo $INIT_CWD && src/lib/assets/scripts/install.js",
    "sample_apostinstall": "cp -p src/lib/assets/icons $INIT_CWD/assets/icons",
    "sample_uninstall": "src/lib/assets/scripts/uninstall.js",
    "table:version": "npm run build --prefix projects/logo-software/table/",
    "sample_node_trigger": " node -e 'console.log(process.env)'"
  },
