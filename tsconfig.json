{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "rootDir": "./",
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [],
    "paths": {
      "@paas/*": [
        "./projects/developer.logo.com.tr/*"
      ],
      "@samples/*": [
        "./projects/samples/src/app/logo/*"
      ],
      "@logo-software/core": [
        "./dist/logo-software/core"
      ],
      "@logo-software/language": [
        "./dist/logo-software/language"
      ],
      "@logo-software/excel": [
        "./dist/logo-software/excel"
      ],
      "@logo-software/paging": [
        "./dist/logo-software/paging"
      ],
      "@logo-software/table": [
        "./dist/logo-software/table"
      ],
      "@logo-software/toast": [
        "dist/logo-software/toast"
      ],
      "@logo-software/icons": [
        "dist/logo-software/icons"
      ],
      "@logo-software/theme": [
        "dist/logo-software/theme"
      ],
      "@logo-software/input": [
        "dist/logo-software/input"
      ],
      "@logo-software/*": [
        "./projects/logo-software/*"
      ],
      "@logo-software/perspective": [
        "dist/logo-software/perspective/logo-software-perspective",
        "dist/logo-software/perspective"
      ],
      "@logo-software/cursor": [
        "dist/logo-software/cursor/logo-software-cursor",
        "dist/logo-software/cursor"
      ],
      "@logo-software/carousel": [
        "dist/logo-software/carousel/logo-software-carousel",
        "dist/logo-software/carousel"
      ],
      "@logo-software/link": [
        "dist/logo-software/link/logo-software-link",
        "dist/logo-software/link"
      ],
      "@logo-software/combobox": [
        "dist/logo-software/combobox/logo-software-combobox",
        "dist/logo-software/combobox"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "enableIvy": false
  }
}
