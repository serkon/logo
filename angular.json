{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "documentation": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/documentation",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/app/example",
              {
                "glob": "versions.json",
                "input": "src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "projects/playground/src/app",
                "output": "assets/examples"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@logo-software/icons/src/lib/assets/icons",
                "output": "assets/icons"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@logo-software/toast/src/assets/sounds",
                "output": "assets/sounds"
              }
            ],
            "styles": [
              "src/styles.scss",
              "src/app/@theme/styles/styles.scss",
              "node_modules/pace-js/templates/pace-theme-flash.tmpl.css",
              "node_modules/docsearch.js/dist/cdn/docsearch.min.css",
              "node_modules/highlight.js/styles/dracula.css"
            ],
            "scripts": [
              "node_modules/pace-js/pace.min.js",
              "node_modules/docsearch.js/dist/cdn/docsearch.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "7mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "2mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "port": 4200,
            "browserTarget": "documentation:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "documentation:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "documentation:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "**/*.mp3"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "documentation:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "documentation:serve:production"
            }
          }
        }
      }
    },
